{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://localhost:3000/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "http://localhost:5173/$1"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm run install:all"
}
